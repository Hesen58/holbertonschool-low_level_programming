#include "main.h"
/**
 * print_sign - Something usefull
 * @n: Something more usefull
 *
 * Return: Something much more usefull
 */
unsigned int _strspn(char *s, char *accept)
{
  int i = 0, j = 0, a = 0;

  while (*(s + i) != ' ' && *(s + i) != '\0')
	{
	  for (;*(accept + j) != '\0'; j++)
		{
		  if (*(s + i) == *(accept + j))
			{
			  a++;
			}
		}
	  i++;
	}
  return (a);
}
